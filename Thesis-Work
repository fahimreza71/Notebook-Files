ations\n","for i in range(len(models)):\n","    plt.text(text_accuracies[i] + 0.5, i - bar_height/2, f\"{text_accuracies[i]:.2f}%\", va='center')\n","    plt.text(structured_accuracies[i] + 0.5, i + bar_height/2, f\"{structured_accuracies[i]:.2f}%\", va='center')\n","\n","plt.grid(axis='x', linestyle='--', alpha=0.6)\n","plt.tight_layout()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":607},"id":"b0Y1MkX7pFWy","executionInfo":{"status":"ok","timestamp":1744639919962,"user_tz":-360,"elapsed":1820,"user":{"displayName":"Fahim Reza","userId":"02056703871703027106"}},"outputId":"6157b157-4046-42bb-e513-c2cd90ee105b"},"execution_count":1,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["# prompt: Print the above bar chart in different attractive style, i don't like the above style\n","\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Model names\n","models = [\"Logistic Regression\", \"LightGBM\", \"Random Forest\", \"Ensemble (Soft Voting)\"]\n","\n","# Accuracy values\n","text_accuracies = [82.00, 79.00, 77.00, 81.14]\n","structured_accuracies = [87.00, 98.00, 98.00, 98.32]\n","\n","# Set bar position\n","y_pos = np.arange(len(models))\n","bar_width = 0.35\n","\n","# Create figure and axes\n","fig, ax = plt.subplots(figsize=(12, 6))\n","\n","# Create bars\n","rects1 = ax.barh(y_pos - bar_width / 2, text_accuracies, bar_width, label='Text-Based', color='#1f77b4', edgecolor='black')\n","rects2 = ax.barh(y_pos + bar_width / 2, structured_accuracies, bar_width, label='Structured Data', color='#ff7f0e', edgecolor='black')\n","\n","# Add text labels to the bars\n","def autolabel(rects):\n","    for rect in rects:\n","        width = rect.get_width()\n","        ax.text(width + 0.5, rect.get_y() + rect.get_height() / 2, f\"{width:.2f}%\", ha='left', va='center', fontsize=10, color='black')\n","\n","autolabel(rects1)\n","autolabel(rects2)\n","\n","# Customize plot\n","ax.set_yticks(y_pos)\n","ax.set_yticklabels(models, fontsize=12)\n","ax.invert_yaxis()  # labels read top-to-bottom\n","ax.set_xlabel('Accuracy (%)', fontsize=12)\n","ax.set_title('Accuracy Comparison of Classification Models', fontsize=14)\n","ax.set_xlim(0, 105)  # Adjust x-axis limits\n","ax.legend(loc='lower right', fontsize=12)\n","ax.grid(axis='x', linestyle='--', alpha=0.7)\n","ax.spines['top'].set_visible(False)\n","ax.spines['right'].set_visible(False)\n","\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":607},"id":"hFUMQ54tpjgJ","executionInfo":{"status":"ok","timestamp":1744640212747,"user_tz":-360,"elapsed":398,"user":{"displayName":"Fahim Reza","userId":"02056703871703027106"}},"outputId":"599b8cb7-4abd-418e-f3e3-1672802ce4e4"},"execution_count":4,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["# prompt: Good. But the legends are above the bar. Move it to a different position and give space between 2 bar for same model. Keep others all same\n","\n","import matplotlib.pyplot as plt\n","import numpy as np\n","\n","# Model names (common across both input types)\n","models = [\"Random Forest\", \"Logistic Regression\", \"LightGBM\", \"Ensemble (Soft Voting)\"]\n","\n","# Accuracy values\n","text_accuracies = [77.00, 82.00, 79.00, 81.14]\n","structured_accuracies = [98.00, 87.00, 98.00, 98.32]\n","\n","# Set bar position\n","y_pos = np.arange(len(models))\n","bar_width = 0.35\n","\n","# Create figure and axes\n","fig, ax = plt.subplots(figsize=(12, 6))\n","\n","# Create bars with space between groups\n","rects1 = ax.barh(y_pos - bar_width / 2, text_accuracies, bar_width, label='Text Data', color='#1f77b4', edgecolor='black')\n","rects2 = ax.barh(y_pos + bar_width / 2, structured_accuracies, bar_width, label='Structured Data', color='#ff7f0e', edgecolor='black')\n","\n","# Add text labels to the bars\n","def autolabel(rects):\n","    for rect in rects:\n","        width = rect.get_width()\n","        ax.text(width + 0.5, rect.get_y() + rect.get_height() / 2, f\"{width:.2f}%\", ha='left', va='center', fontsize=10, color='black')\n","\n","autolabel(rects1)\n","autolabel(rects2)\n","\n","# Customize plot\n","ax.set_yticks(y_pos)\n","ax.set_yticklabels(models, fontsize=12)\n","ax.invert_yaxis()  # labels read top-to-bottom\n","ax.set_xlabel('Accuracy (%)', fontsize=12)\n","ax.set_title('Accuracy Comparison of Classification Models', fontsize=14)\n","ax.set_xlim(0, 110)  # Adjust x-axis limits\n","# Move the legend to the upper right\n","ax.legend(loc='best', fontsize=10)\n","ax.grid(axis='x', linestyle='--', alpha=0.7)\n","ax.spines['top'].set_visible(False)\n","ax.spines['right'].set_visible(False)\n","\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":607},"id":"_HGbhxehqR9H","executionInfo":{"status":"ok","timestamp":1744640794410,"user_tz":-360,"elapsed":420,"user":{"displayName":"Fahim Reza","userId":"02056703871703027106"}},"outputId":"46b6f5e5-cf42-433a-9138-3a90e16ddd88"},"execution_count":14,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import pandas as pd\n","\n","# Load the dataset\n","file_path = \"final_dataset.csv\"\n","df = pd.read_csv(file_path)\n","\n","# Define the columns to include in the text column\n","columns_to_include = [\n","    \"energy_100g\", \"saturated-fat_100g\", \"sugars_100g\", \"fiber_100g\",\n","    \"proteins_100g\", \"salt_100g\", \"fruits-vegetables-nuts-estimate-from-ingredients_100g\"\n","]\n","\n","# Populate the 'text' column without extra decimal points\n","df[\"text\"] = df[columns_to_include].apply(lambda row: ','.join(f\"{col}:{int(row[col]) if row[col] % 1 == 0