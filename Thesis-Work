\": [\"Accuracy\", \"Precision\", \"Recall\", \"F1-score\"],\n","    \"Value\": [accuracy, precision, recall, f1]\n","}\n","\n","# Convert to Pandas DataFrame\n","import pandas as pd\n","df_metrics = pd.DataFrame(metrics)\n","\n","# 📊 Bar Plot using Seaborn\n","plt.figure(figsize=(6, 4))\n","sns.barplot(x=\"Metric\", y=\"Value\", data=df_metrics, palette=\"Blues\", edgecolor=\"black\")\n","\n","# Add values on bars\n","for i, v in enumerate(df_metrics[\"Value\"]):\n","    plt.text(i, v + 0.02, f\"{v:.2f}\", ha=\"center\", fontsize=12)\n","\n","# Formatting\n","plt.ylim(0, 1.1)\n","plt.title(\"Model Performance Metrics for Logistic Regression\")\n","plt.ylabel(\"Score\")\n","plt.xlabel(\"\")\n","\n","# Show plot\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":498},"id":"zJn11lgU5K5R","executionInfo":{"status":"ok","timestamp":1738671945808,"user_tz":-360,"elapsed":766,"user":{"displayName":"Fahim Reza","userId":"02056703871703027106"}},"outputId":"7bb19364-8aa2-4be7-dae1-f1f22e44bbad"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stderr","text":["<ipython-input-12-2afde21448cf>:30: FutureWarning: \n","\n","Passing `palette` without assigning `hue` is deprecated and will be removed in v0.14.0. Assign the `x` variable to `hue` and set `legend=False` for the same effect.\n","\n","  sns.barplot(x=\"Metric\", y=\"Value\", data=df_metrics, palette=\"Blues\", edgecolor=\"black\")\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 600x400 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["# BERT Model"],"metadata":{"id":"g4Esz3VgREjz"}},{"cell_type":"code","source":["pip install datasets"],"metadata":{"id":"OBqn6xPJ718J"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["import pandas as pd\n","from transformers import BertTokenizer, BertForSequenceClassification, Trainer, TrainingArguments\n","from sklearn.model_selection import train_test_split\n